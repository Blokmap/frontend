{
    "name": "frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc -b && vite build",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "format": "prettier --write 'src/**/*.{ts,vue,css,scss,md}'",
        "preview": "vite preview",
        "test:unit": "vitest run",
        "test:e2e": "cypress run"
    },
    "dependencies": {
        "vue": "^3.5.13"
    },
    "devDependencies": {
        "@eslint/js": "^9.21.0",
        "@typescript-eslint/eslint-plugin": "^8.24.1",
        "@typescript-eslint/parser": "^8.24.1",
        "cypress": "^14.0.3",
        "eslint": "^9.21.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-vue": "^9.32.0",
        "prettier": "^3.5.2",
        "typescript": "~5.7.2",
        "typescript-eslint": "^8.24.1",
        "vite": "^6.1.1",
        "vitest": "^3.0.6",
        "vue-tsc": "^2.2.0"
    }
}
