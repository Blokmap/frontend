{
    "name": "frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc -b && vite build",
        "lint": "eslint --ext .js,.ts,.vue src",
        "lint:fix": "eslint --ext .js,.ts,.vue --fix src",
        "format": "prettier --write 'src/**/*.{js,ts,vue,css,scss,md}'",
        "preview": "vite preview",
        "test:unit": "vitest run",
        "test:e2e": "cypress run"
    },
    "dependencies": {
        "vue": "^3.5.13"
    },
    "devDependencies": {
        "@eslint/js": "^9.21.0",
        "@types/cypress": "^0.1.6",
        "@types/node": "^22.13.5",
        "@typescript-eslint/eslint-plugin": "^8.24.1",
        "@typescript-eslint/parser": "^8.24.1",
        "@vitejs/plugin-vue": "^1.7.1",
        "@vue/tsconfig": "^0.7.0",
        "cypress": "^14.0.3",
        "eslint": "^9.21.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-vue": "^9.32.0",
        "prettier": "^3.5.2",
        "typescript": "~5.7.2",
        "vite": "^6.1.1",
        "vitest": "^3.0.6",
        "vue-tsc": "^2.2.0"
    }
}
