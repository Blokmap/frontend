{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "compilerOptions": {
        "types": ["vite/client", "geojson"],
        "strict": true,
        "noImplicitAny": true,
        "forceConsistentCasingInFileNames": true,
        "paths": {
            "@/*": ["./src/*"]
        }
    },
    "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.vue", "test/**/*.ts"]
}
